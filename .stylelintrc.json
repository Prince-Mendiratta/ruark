{
	"extends": "stylelint-config-standard",
	"rules": {
		"at-rule-no-unknown": [
			true,
			{
				"ignoreAtRules": [
					"tailwind",
					"apply",
					"layer",
					"variants",
					"responsive",
					"screen"
				]
			}
		],
		"at-rule-no-deprecated": null,
		"no-descending-specificity": null
	}
}